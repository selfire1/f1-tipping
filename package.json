{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "scripts": {
    "prebuild": "bun run scripts/save-avatars.ts",
    "build": "GIT_COMMIT=`git log -1 --pretty='%aD %B'` nuxt build",
    "dev": "GIT_COMMIT=`git log -1 --pretty='%aD %B'` nuxt dev",
    "dev:staging": "nuxt dev --dotenv .env.staging",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "db:generate": "drizzle-kit generate",
    "db:generate:staging": "bun --env-file=.env.staging run db:generate",
    "db:migrate:staging": "bun --env-file=.env.staging run db:migrate",
    "db:push:staging": "bun --env-file=.env.staging run db:push",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:seed": "bun run ./server/db/seed.ts",
    "db:studio": "drizzle-kit studio",
    "db:dump": "turso db shell f1-tipping .dump > dump.sql && rm local.db && cat dump.sql | sqlite3 local.db",
    "www:prod": "open 'https://dash.cloudflare.com/4d5f60f30043f65cd1964f903d344b72/pages/view/f1-tipping'",
    "save-images": "bun run scripts/save-avatars.ts",
    "test": "vitest",
    "test:watch": "vitest --watch"
  },
  "dependencies": {
    "@libsql/client": "^0.15.4",
    "@libsql/kysely-libsql": "^0.4.1",
    "@nuxt/fonts": "0.11.2",
    "@nuxt/icon": "^1.12.0",
    "@nuxt/image": "^1.10.0",
    "@nuxt/ui": "3.1.0",
    "@paralleldrive/cuid2": "^2.2.2",
    "@vueuse/core": "^13.1.0",
    "@vueuse/router": "^13.1.0",
    "bcryptjs": "^3.0.2",
    "better-auth": "^1.2.7",
    "date-fns": "^4.1.0",
    "dotenv": "^16.5.0",
    "drizzle-orm": "^0.43.1",
    "nuxt": "^3.17.1",
    "pug": "^3.0.3",
    "vue": "^3.5.13",
    "zod": "^3.24.3"
  },
  "devDependencies": {
    "@iconify-json/carbon": "^1.2.8",
    "@nuxt/eslint-config": "^1.3.0",
    "@nuxt/test-utils": "^3.17.2",
    "@prettier/plugin-pug": "^3.4.0",
    "@tailwindcss/typography": "^0.5.16",
    "@types/bun": "^1.2.11",
    "@typescript-eslint/eslint-plugin": "^8.31.1",
    "@typescript-eslint/parser": "^8.31.1",
    "@vue/language-plugin-pug": "^2.2.10",
    "@vue/test-utils": "^2.4.6",
    "drizzle-kit": "^0.31.0",
    "happy-dom": "^17.4.6",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "typescript": "^5.8.3",
    "vitest": "^3.1.2"
  }
}
